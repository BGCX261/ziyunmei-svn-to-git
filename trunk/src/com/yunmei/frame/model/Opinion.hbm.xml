<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.yunmei.frame.model.Opinion" table="sys_opinion">
		<id name="id" column="id_">
			<generator class="native"></generator>
		</id>
		<property name="date" column="date_" />
		<property name="businessId" column="business_id_" />
		<property name="opinion" column="opinion_" />
		<property name="key" column="key_" />
		<property name="result" column="result_" />
		<property name="taskId" column="task_id_" />
		<property name="taskName" column="task_name_" />
		<property name="userId" column="user_id_" />
		<property name="userName" column="user_name_" />
		
	</class>
</hibernate-mapping>
