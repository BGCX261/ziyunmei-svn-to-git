<module name="logging" class="com.sinosoft.frame.axis2.module.LoggingModule">
	<InFlow>
		<handler name="InFlowLogHandler" class="com.sinosoft.frame.axis2.module.LogHandler">
			<order phase="loggingPhase" />
		</handler>
	</InFlow>
	<OutFlow>
		<handler name="OutFlowLogHandler" class="com.sinosoft.frame.axis2.module.LogHandler">
			<order phase="loggingPhase" />
		</handler>
	</OutFlow>
	<OutFaultFlow>
		<handler name="FaultOutFlowLogHandler" class="com.sinosoft.frame.axis2.module.LogHandler">
			<order phase="loggingPhase" />
		</handler>
	</OutFaultFlow>
	<InFaultFlow>
		<handler name="FaultInFlowLogHandler" class="com.sinosoft.frame.axis2.module.LogHandler">
			<order phase="loggingPhase" />
		</handler>
	</InFaultFlow>
</module>